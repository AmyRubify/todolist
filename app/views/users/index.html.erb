<h1>User List</h1>
<table>
  <thead>
    <th>
      UserName
    </th>
    <th>
      Name
    </th>
  </thead>

    <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'show', user_url(user.id) %></td>
    </tr>
    <% end %>

</table>